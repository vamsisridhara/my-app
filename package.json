{
  "name": "my-app",
  "version": "0.0.0",
  "engineStrict": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build-chart-lib": "ng build my-app-lib --prod --build-optimizer false --output-hashing all",
    "prodbuild": "./build.sh production",
    "qabuild": "./build.sh qa",
    "read-xml": "node readxml.js",
    "run-fr": "ng serve --configuration=fr",
    "run-en": "ng serve --configuration=en",
    "gen-locale-en": "ng xi18n --output-path --out-file i18n/locale/my-app.en.xlf",
    "gen-locale-fr": "ng xi18n --output-path --out-file i18n/locale/my-app.fr.xlf",
    "build-fr-i18n": "ng build --prod --i18n-file i18n/locale/my-app.fr.xlf --i18n-format xlf --i18n-locale fr",
    "build-en-i18n": "ng build --prod --i18n-file i18n/locale/my-app.en.xlf --i18n-format xlf --i18n-locale en"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.14",
    "@angular/cdk": "^8.0.2",
    "@angular/common": "~8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/core": "~8.2.14",
    "@angular/forms": "~8.2.14",
    "@angular/http": "~7.2.15",
    "@angular/material": "^8.0.2",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "@angular/router": "~8.2.14",
    "@sweetalert2/ngx-sweetalert2": "~5.1.0",
    "@types/lodash-es": "~4.17.3",
    "bootstrap": "~4.3.1",
    "chart.js": "~2.8.0",
    "exceljs": "^3.4.0",
    "gzipper": "~2.8.0",
    "hammerjs": "^2.0.8",
    "lodash-es": "~4.17.11",
    "moment": "~2.24.0",
    "ng2-charts": "~2.3.0",
    "ngx-device-detector": "^1.3.6",
    "rxjs": "~6.5.4",
    "rxjs-compat": "~6.5.2",
    "sweetalert2": "~8.13.0",
    "tslib": "^1.10.0",
    "xml-js": "^1.6.11",
    "xml2js": "~0.4.22",
    "xmldom": "latest",
    "zone.js": "~0.9.1",
    "ngx-take-until-destroy": "~5.4.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.25",
    "@angular-devkit/build-ng-packagr": "~0.803.25",
    "@angular/cli": "~8.3.25",
    "@angular/compiler-cli": "~8.2.14",
    "@angular/language-service": "~8.2.14",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ng-packagr": "^5.7.1",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tsickle": "^0.35.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3"
  }
}